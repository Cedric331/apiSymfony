{% extends 'base.html.twig' %}

{% block title %}Hello ApiController!{% endblock %}

{% block body %}
<div class="container">
<div class="row">
   {% for region in regions %}
   <div class="col-md-5 my-2 mx-2">
      <div class="card" style="width: 18rem;">
        <div class="card-body">
          <h5 class="card-title"> {{ region.code }} </h5>
          <p class="card-text">{{ region.nom }}</p>
        </div>
      </div>
   </div>
   {% endfor %}
</div>
</div>
{% endblock %}
